
<div image_gallery style="float:left;width: 100%;height: 100%;margin: 35px 0px 20px 50px">
  <div image_gallery style="float:left;width: 100%;height: 100%;">
  <%= link_to 'Posts Portion', :controller => "posts", :action => 'index', :style => "float:left;width:100%;"%>
    </div>
  <div image_gallery style="float:left;width: 100%;height: 100%;">
  <%= link_to 'Upload New Picture', :controller => "pictures", :action => 'new', :style => 'float:leftwidth:100%;' %>
  </div>
  </div>

<% @pictures.each do |a| %>
    <div image_gallery style="float:left;width: 40%;height: 40%;margin: 35px 0px 20px 50px">

      <%= image_tag a.pic.url(:thumb) %>
      <%#=  link_to "Delete Piucture",  %>
      <%= link_to "Delete", picture_path(a), method: :delete, data: {confirm: "Are You Sure"}  %>

    </div>
<% end %>
